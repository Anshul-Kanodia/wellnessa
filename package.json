{
  "name": "figma-replica",
  "version": "1.0.0",
  "description": "Wellnessa Healthcare Platform - React/Node.js Assessment System",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["healthcare", "assessment", "react", "nodejs", "wellnessa"],
  "author": "Wellnessa Team",
  "license": "MIT"
}