{
  "name": "figma-replica",
  "version": "1.0.0",
  "description": "Wellnessa Healthcare Platform - React/Node.js Assessment System",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "seed": "node server/firebase-seed.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "firebase-admin": "^12.0.0",
    "firebase": "^10.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "healthcare",
    "assessment",
    "react",
    "nodejs",
    "wellnessa"
  ],
  "author": "Wellnessa Team",
  "license": "MIT"
}
